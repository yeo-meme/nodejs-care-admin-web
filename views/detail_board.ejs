 <div id="layoutSidenav_content">
  <main>
   <div class="container-fluid">
    <h1 class="mt-4">주문상세정보</h1>
    <ol class="breadcrumb mb-4">
     <li class="breadcrumb-item"><a href="/adminboard">전체신규주문</a></li>
     <li class="breadcrumb-item active">주문상세정보</li>
    </ol>
    <div class="card mb-4">
     <div class="card-body">


      <!--form-->
      <form role="form" id="sending_delivery"  method="post" autocomplete="on">
       <!--임시 저장소-->
       <input type="hidden"  name="deliveryNumb" id="deliveryNumb" value=""/>
       <input type="hidden"  name="userDocId" id="userDocId" value=""/>
      </form>


      <caption>상품주문정보 조회</caption>
      <table border="1">

       <tbody>
       <!--데이터 반복시작-->

        <tr align="center">
        <th bgcolor="#D4F4FA">상품명</th>
        <td colspan="4"><%= snapshot.data().productName%></td>
       </tr>
       <tr align="center">
        <th bgcolor="#D4F4FA">주문상태</th>
        <td ><%= snapshot.data().orderStatus%></td>
        <td ><select name="deliveryCom">
         <option value="cj">CJ 대한통운 </select></td>
        <td ><input type="text" name="deliveryNum">
         <input class="delivery_button" type="button" onclick="printDeliveryNumb('<%= selectedId %>');" value="발주확인"/>
        </td>
       </tr>

       <tr align="center" >
        <th bgcolor="#D4F4FA">구매자명</th>
        <td ><%= snapshot.data().orderer%></td>
        <td bgcolor="#D4F4FA">구매자 ID</td>
        <td ><%= snapshot.data().ordererId%></td>
       </tr>
       <tr align="center" >
        <th bgcolor="#D4F4FA">옵션</th>
        <th><%= snapshot.data().option%></th>
        <th bgcolor="#D4F4FA">주문수량</th>
        <th><%= snapshot.data().amount%></th>
       </tr>
       <tr align="center">
        <th bgcolor="#D4F4FA">총 상품금액</th>
        <th colspan="4"><%= snapshot.data().totalPrice%></th>
       </tr>
       </tbody>
      </table>


      <caption>배송지 정보</caption>
      <table border="1" >
       <tbody>

       <tr align="center" >
       <th bgcolor="#D4F4FA">수취인명</th>
       <th ><% snapshot.data().receiver%></th>
       </tr>
       <tr align="center" >
       <th bgcolor="#D4F4FA">연락처</th>
       <th ><% snapshot.data().ordererPhone%></th>
       </tr>
       <tr align="center">
       <th bgcolor="#D4F4FA">배송지</th>
       <th ><% snapshot.data().ordererAddress%></th>
       </tr>
       <tr align="center" >
       <th bgcolor="#D4F4FA">배송메모</th>
       <th ><% snapshot.data().receiverMemo %></th>
       </tr>
       </tbody>

      </table>
<!--      <label for="jb-input-text">운송장번호</label>-->
<!--      <select name="delivery_company">-->
<!--       <option value="">CJ대한통운</option>-->
<!--      </select>-->
<!--      <input type="text" id="deliveryNumberTx" >-->
      <div>
       <input class="delivery_button" type="button" onclick="printDeliveryNumb('<%= selectedId %>');" value="발주확인"/>
      </div>
