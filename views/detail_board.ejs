<script>
 function printDeliveryNumb(id) {
  alert('클릭 :' + id);

  var form = document.getElementById('sending_delivery');
  // var setValue = document.getElementById('deliveryNumb');
  // setValue.value = document.getElementById("deliveryNumberTx").value;

  var userDoc = document.getElementById('userDocId');
  userDoc.value  = id;

  form.action = "/adminboard";
  form.method =  "POST";
  form.submit();

 }
 
</script>
